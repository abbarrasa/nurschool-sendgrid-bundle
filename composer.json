{
    "name": "nurschool/sendgrid-bundle",
    "description": "Symfony bundle integrating Sendgrid REST API for Nurschool project",
    "type": "symfony-bundle",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.2.5",
        "sendgrid/sendgrid": "^7.9"
    },
    "license": "AGPL-3.0-only",
    "autoload": {
        "psr-4": {
            "Nurschool\\Bundle\\NurschoolSendgridBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": { "Nurschool\\Bundle\\NurschoolSendgridBundle\\Tests\\": "tests/" }
    },
    "authors": [
        {
            "name": "Alberto Buitrago Barrasa",
            "email": "abbarrasa@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "path",
            "url": "NurschoolSendgridBundle"
        }
    ]
}
